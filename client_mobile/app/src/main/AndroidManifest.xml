<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.area">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.example.area.TrelloActivity"
            android:label="@string/title_activity_trello"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.example.area.MarsActivity"
            android:label="@string/title_activity_mars"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.NasaActivity"
            android:label="@string/title_activity_nasa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.SteamActivity"
            android:label="@string/title_activity_steam"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.PollutionActivity"
            android:label="@string/title_activity_pollution"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.WeatherActivity"
            android:label="@string/title_activity_weather"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.NYActivity"
            android:label="@string/title_activity_ny"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.LolActivity"
            android:label="@string/title_activity_lol"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.BitcoinActivity"
            android:label="Area : Bitcoin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.MainActivity"
            android:label="Area"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.area.SecondActivity"
            android:label="Area"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.area.RegisterActivity"
            android:label="Area"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>